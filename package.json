{
  "name": "galaxy-density-field",
  "version": "1.0.0",
  "description": "Three.js密度函数引力场银河系效果 - React现代化重构版",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:external": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "server": "cd src/server && npm start",
    "server:dev": "cd src/server && npm run dev",
    "server:test": "cd src/server && npm test",
    "start:all": "concurrently \"npm run server\" \"npm run dev\"",
    "start:auto": "powershell -ExecutionPolicy Bypass -File scripts/start-with-auto-port.ps1",
    "start:auto:verbose": "powershell -ExecutionPolicy Bypass -File scripts/start-with-auto-port.ps1 -Verbose",
    "start:simple": "powershell -ExecutionPolicy Bypass -File scripts/start-simple.ps1",
    "install:server": "cd src/server && npm install",
    "models:index": "node scripts/generate-model-index.js",
    "models:scan": "node scripts/generate-model-index.js",
    "models:watch": "node scripts/watch-models.js",
    "models:test": "node scripts/test-model-detection.js"
  },
  "dependencies": {
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/postprocessing": "^2.15.0",
    "better-sqlite3": "^11.10.0",
    "lil-gui": "^0.19.1",
    "postprocessing": "^6.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "three.js",
    "react",
    "visualization",
    "3d",
    "galaxy",
    "density-field",
    "gravitational-field"
  ],
  "author": "约翰",
  "license": "MIT"
}
